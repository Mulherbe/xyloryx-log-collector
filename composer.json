{
    "name": "xyloryx/log-collector",
    "description": "Laravel error collector package for Xyloryx Log monitoring system",
    "version": "1.2.6",
    "type": "library",
    "license": "proprietary",
    "authors": [
        {
            "name": "Xyloryx",
            "email": "contact@xyloryx.com"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/http": "^10.0|^11.0|^12.0",
        "illuminate/routing": "^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Xyloryx\\LogCollector\\": "src/"
        }
    },
    "extra": {
        "laravel": {
