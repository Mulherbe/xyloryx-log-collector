{
    "name": "xiloryx/log-collector",
    "description": "Laravel error collector package for Xiloryx Log monitoring system",
    "version": "1.3.0",
    "type": "library",
    "license": "proprietary",
    "authors": [
        {
            "name": "Xiloryx",
            "email": "contact@xiloryx.com"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/http": "^10.0|^11.0|^12.0",
        "illuminate/routing": "^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Xiloryx\\LogCollector\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Xiloryx\\LogCollector\\XiloryxLogServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
